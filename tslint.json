{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "ordered-imports": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "prefer-for-of": false,
    "no-console": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
