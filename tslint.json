{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
